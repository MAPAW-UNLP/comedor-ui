{
	"$schema": "./node_modules/@angular/cli/lib/config/schema.json",
	"version": 1,
	"newProjectRoot": "projects",
	"projects": {
		"comedor-ui": {
			"projectType": "application",
			"schematics": {
				"@schematics/angular:component": {
					"style": "scss"
				}
			},
			"root": "",
			"sourceRoot": "src",
			"prefix": "cu",
			"architect": {
				"build": {
					"builder": "@angular-devkit/build-angular:browser",
					"options": {
						"outputPath": "dist/comedor-ui",
						"index": "src/index.html",
						"main": "src/main.ts",
						"polyfills": "src/polyfills.ts",
						"tsConfig": "tsconfig.app.json",
						"aot": true,
						"assets": [
							"src/favicon.ico",
							"src/assets"
						],
						"styles": [
							"src/styles.scss"
						],
						"scripts": [],
						"fileReplacements": [
							{
								"replace": "src/environments/environment.ts",
								"with": "src/environments/environment.local.ts"
							}
						]
					},
					"configurations": {
						"local": {
							"fileReplacements": [
								{
									"replace": "src/environments/environment.ts",
									"with": "src/environments/environment.local.ts"
								}
							]
						},
						"remote": {
							"fileReplacements": [
								{
									"replace": "src/environments/environment.ts",
									"with": "src/environments/environment.remote.ts"
								}
							]
						}
					}
				},
				"serve": {
					"builder": "@angular-devkit/build-angular:dev-server",
					"options": {
						"browserTarget": "comedor-ui:build",
						"proxyConfig": "src/proxy.conf.json"
					},
					"configurations": {
						"local": {
							"browserTarget": "comedor-ui:build:local",
							"proxyConfig": "src/proxy.conf.json"
						},
						"remote": {
							"browserTarget": "comedor-ui:build:remote",
							"proxyConfig": "src/proxy.conf.json"
						}
					}
				},
				"extract-i18n": {
					"builder": "@angular-devkit/build-angular:extract-i18n",
					"options": {
						"browserTarget": "comedor-ui:build"
					}
				},
				"test": {
					"builder": "@angular-builders/jest:run",
					"options": {
						"polyfills": "src/polyfills.ts",
						"tsConfig": "tsconfig.spec.json",
						"assets": [
							"src/favicon.ico",
							"src/assets"
						],
						"styles": [
							"src/styles.scss"
						],
						"scripts": []
					}
				},
				"lint": {
					"builder": "@angular-devkit/build-angular:tslint",
					"options": {
						"tsConfig": [
							"tsconfig.app.json",
							"tsconfig.spec.json",
							"cypress/tsconfig.json"
						],
						"exclude": [
							"**/node_modules/**"
						]
					}
				},
				"e2e": {
					"builder": "@nrwl/cypress:cypress",
					"options": {
						"cypressConfig": "cypress.json",
						"tsConfig": "cypress/tsconfig.json"
					}
				}
			}
		}
	},
	"defaultProject": "comedor-ui",
	"cli": {
		"analytics": false
	}
}