{
  "name": "comedor-ui",
  "version": "0.0.0",
  "scripts": {
    "start": "ng serve",
    "lint": "ng lint --fix",
    "unit": "ng test",
    "unit:watch": "ng test --watch",
    "e2e": "concurrently \"wait-on http-get://localhost:5200 && ng e2e --headless\" \"ng serve --configuration=local --port=5200\" -r -k -s first",
    "e2e:interactive": "concurrently \"wait-on http-get://localhost:5200 && ng e2e --watch\" \"ng serve --configuration=local --port=5200\" -r -k -s first",
    "test": "npm run lint && npm run unit && npm run e2e",
    "postinstall": "ngcc"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~10.1.2",
    "@angular/cdk": "^10.2.1",
    "@angular/common": "~10.1.2",
    "@angular/compiler": "~10.1.2",
    "@angular/core": "~10.1.2",
    "@angular/forms": "~10.1.2",
    "@angular/material": "^10.2.1",
    "@angular/platform-browser": "~10.1.2",
    "@angular/platform-browser-dynamic": "~10.1.2",
    "@angular/router": "~10.1.2",
    "@auth0/angular-jwt": "^5.0.1",
    "rxjs": "~6.6.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-builders/jest": "^10.0.1",
    "@angular-devkit/build-angular": "~0.1001.2",
    "@angular/cli": "~10.1.2",
    "@angular/compiler-cli": "~10.1.2",
    "@nrwl/cypress": "^10.2.0",
    "@nrwl/workspace": "^10.2.0",
    "@types/jest": "^26.0.14",
    "@types/node": "^12.11.1",
    "codelyzer": "^6.0.0",
    "concurrently": "^5.3.0",
    "cypress": "^5.2.0",
    "jest": "^26.4.2",
    "ts-node": "~8.3.0",
    "tsdef": "0.0.14",
    "tslint": "~6.1.0",
    "typescript": "~4.0.2",
    "wait-on": "^5.2.0"
  }
}
