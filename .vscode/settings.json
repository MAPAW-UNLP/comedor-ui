		"EXPLAIN": {
			"icon": "info",
			"iconColour": "yellow",
			"rulerColour": "yellow",
			"background": "a08000"
		},
		"FIX": {
			"icon": "flame",
			"iconColour": "red",
			"rulerColour": "red",
			"background": "ff0000"
		},
		"REFACTOR": {
			"icon": "settings",
			"iconColour": "orange",
			"rulerColour": "orange",
			"background": "ff8000"
		},
		"REMOVE": {
			"icon": "trashcan",
			"iconColour": "purple",
			"rulerColour": "purple",
			"background": "800080"
		},
		"TEST": {
			"icon": "beaker",
			"iconColour": "green",
			"rulerColour": "green",
			"background": "008000"
		},
