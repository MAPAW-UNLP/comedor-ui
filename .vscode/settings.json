{
	"workbench.colorCustomizations": {
		"statusBar.background": "#9047b8",
		"activityBar.activeBorder": "#c8a4dbc0",
		"activityBar.activeBackground": "#9047b840",
		"activityBarBadge.background": "#9047b8"
	},
	"todo-tree.general.tags": [
		"DO",
		"EXPLAIN",
		"FIX",
		"REFACTOR",
		"REMOVE",
		"TEST"
	],
	"todo-tree.highlights.defaultHighlight": {
		"type": "tag-and-comment",
		"foreground": "white",
		"opacity": "100",
		"fontWeight": "bold"
	},
	"todo-tree.highlights.customHighlight": {
		"DO": {
			"icon": "check",
			"iconColour": "blue",
			"rulerColour": "blue",
			"background": "0000ff"
